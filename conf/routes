# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /			                          	controllers.Application.index()
GET     /legalInformation	                	controllers.Application.legalInformation()
GET		/todos									controllers.Application.todos()
POST	/todos									controllers.Application.newTodo()
POST	/todos/:id								controllers.Application.updateTodo(id: Long)
DELETE	/todos/:id								controllers.Application.deleteTodo(id: Long)